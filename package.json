{
  "name": "babylon-lottie-website",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "dev:raw": "vite --config vite.debug.config.ts",
    "build:raw": "tsc && vite build --config vite.debug.config.ts",
    "build:prod": "tsc && vite build --config vite.prod.config.ts",
    "serve:prod": "vite preview --config vite.prod.config.ts",
    "analyze:prod": "npm run build:prod && vite-bundle-visualizer --template treemap --open true"
  },
  "devDependencies": {
    "cross-env": "^10.1.0",
    "terser": "^5.44.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.7",
    "vite-bundle-visualizer": "^1.2.1"
  },
  "dependencies": {
    "@babylonjs/core": "^8.31.2",
    "@babylonjs/lottie-player": "^8.31.2"
  }
}
